                        <AnchorPane prefHeight="200.0" prefWidth="200.0" />
                    </graphic></Label>
            </children>
        </HBox>
        <Separator prefWidth="200.0" />
        <AnchorPane id="button-entrada" prefHeight="367.0" prefWidth="663.0" stylesheets="@../../../style.css">
            <children>


                <ListView fx:id="listView" layoutX="29.0" layoutY="62.0" prefHeight="242.0" prefWidth="494.0" />
                <Button fx:id="eliminarBoton" alignment="CENTER" contentDisplay="CENTER" layoutX="549.0" layoutY="141.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="350.0" styleClass="boton-menu" text="Eliminar" />
                <Button fx:id="actualizarBoton" alignment="CENTER" contentDisplay="CENTER" layoutX="549.0" layoutY="226.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="350.0" styleClass="boton-menu" text="Actualizar" />
